{
    "flake8.args": ["--max-line-length=160", "--ignore=E501", "--ignore=W503"],
    "python.poetryPath": "poetry",
    "pylint.args": [
        "--max-line-length=160",
        "--disable=too-few-public-methods",
        "--disable=too-many-locals",
        "--disable=too-many-instance-attributes",
        "--disable=unnecessary-pass",
        "--disable=no-name-in-module",
        "--disable=no-else-return",
        "--disable=consider-using-dict-items",
        "--disabe=too-many-nested-blocks"
    ],
    "cSpell.words": [
        "dotenv",
        "JSESSIONID",
        "Newberry",
        "newberryfl",
        "PHPSESSID",
        "Referer",
        "sensus",
        "SITENAME"
    ],
}